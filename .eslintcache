n   \r\n  // 毛玻璃图片的url\r\n  const bgImg = topBanners && topBanners[currentIndex] && topBanners[currentIndex].imageUrl + \"?imageView&blur=40x20\"\r\n  return (\r\n    <TopBannerWrapper bgImage = {bgImg}>\r\n      <div className=\"banner w980\">\r\n        <BannerLeft>\r\n          <Carousel ref={bannerRef} \r\n                    effect=\"fade\"\r\n                    autoplay={true}\r\n                    beforeChange={beforeBannerChange}>\r\n            {\r\n             topBanners && topBanners.map((item,index) =>{\r\n                return (\r\n                  <div key={item.imageUrl}>\r\n                    <img src={item.imageUrl}/>\r\n                  </div>\r\n                )\r\n              })\r\n            }\r\n          </Carousel>\r\n        </BannerLeft>\r\n        <BannerRight>\r\n          <DownLoadButton />\r\n          <DownLoadDesc />\r\n        </BannerRight>\r\n        <BannerControl >\r\n        <button\r\n            className=\"btn\"\r\n            onClick={() => bannerRef.current.prev()}\r\n          ></button>\r\n          <button\r\n            className=\"btn\"\r\n            onClick={() => bannerRef.current.next()}\r\n          ></button>\r\n        </BannerControl >\r\n      </div>\r\n    </TopBannerWrapper>\r\n  )\r\n})\r\n","D:\\React\\music-web-react\\src\\pages\\discover\\c-pages\\recommend\\c-comps\\topBanner\\style.js",[],"D:\\React\\music-web-react\\src\\pages\\discover\\c-pages\\recommend\\style.js",[],{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","severity":1,"message":"136","line":54,"column":21,"nodeType":"137","endLine":54,"endColumn":47},"no-native-reassign",["138"],"no-negated-in-lhs",["139"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]